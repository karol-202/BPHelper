<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:app="http://schemas.android.com/apk/res-auto"
            xmlns:tools="http://schemas.android.com/tools"
            app:startDestination="@id/membersFragment">

    <fragment
            android:id="@+id/membersFragment"
            android:name="pl.karol202.bphelper.ui.MembersFragment"
            android:label="@string/fragment_members"
            tools:layout="@layout/fragment_members">
        <action
                android:id="@+id/toTablesFragment"
                app:destination="@id/tablesFragment"
                app:enterAnim="@anim/nav_default_enter_anim"
                app:exitAnim="@anim/nav_default_exit_anim"
                app:popEnterAnim="@anim/nav_default_pop_enter_anim"
                app:popExitAnim="@anim/nav_default_pop_exit_anim"/>
    </fragment>

    <fragment
            android:id="@+id/tablesFragment"
            android:name="pl.karol202.bphelper.ui.TablesFragment"
            android:label="@string/fragment_tables"
            tools:layout="@layout/fragment_tables_2">
        <argument
                android:name="tableConfigurationType"
                app:argType="string"/>
    </fragment>
</navigation>